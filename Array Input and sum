#include <stdio.h>
#include <stdlib.h>

int main()
{
    int sum=0; int n;
    printf("enter the number of elements: ");
    scanf("%d",&n);
    int x[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&x[i]);
    }
for(int j =0;j<n;j++)
{
    sum=x[j]+sum;
}

printf("sum=%d",sum);


    return 0;
}
